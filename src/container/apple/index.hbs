<div class="page">
	<section class="section header">
		<div class="section__content">
			{{#with header}}
			<header class="header__container">
				<div class="header__block">
					<img class="header__icon" src="/svg/apple.svg" alt="Apple">
					<nav class="header__block">
						{{#each links}}
						<a href="{{url}}" class="header__link">{{text}}</a>
						{{/each}}
					</nav>
				</div>
				<div class="header__block">
					<img class="header__icon" src="/svg/search.svg" alt="Search">
					<img class="header__icon" src="/svg/profile.svg" alt="Profile">
				</div>
			</header>
		</div>
	</section>
	<section class="section ad">
		<div class="section__content">
			<div class="ad__container">
				{{#with ad}}
				<div class="ad__title">
					{{title}}
				</div>
				<div class="ad__action click">
					<a href="{{link.url}}" class="ad__link">{{link.text}}</a>
				</div>
				{{/with}}
			</div>
			{{/with}}
		</div>
	</section>
	<section class="product product-big product--iphone-pro product--dark product--top">
		<div class="product__content">
			{{#with iphone_pro}}
			<h1 class="product__title">{{title}}</h1>
			<p class="product__description">{{descriptions}}</p>
			<div class="product__action">
				{{#with link}}
				{{#each text}}
				<a href="{{../link}}" class="product__link">{{this}}</a>
				{{/each}}
				{{/with}}
			</div>
			{{/with}}
		</div>
	</section>
